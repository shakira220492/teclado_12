                    var userId = data[i].userId;

                    draw_specificListVideo(
                        specificlistvideoId,
                        specificlist,
                        userName,
                        amountVideos,
                        videoId,
                        videoName,
                        videoDescription,
                        videoImage,
                        videoContent,
                        videoUpdatedate,
                        videoAmountViews,
                        videoAmountComments,
                        videoLikes,
                        videoDislikes,
                        userId
                    );

                    i++;
                }
                
